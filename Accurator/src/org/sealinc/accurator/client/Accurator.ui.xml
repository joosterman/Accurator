			<a href="/">
				<img id="headerImage" src="/images/Accurator.png" alt="Accurator" />
			</a>
			<div class="navbar">
				<g:HTMLPanel styleName="logoutBlock" ui:field="dvlogoutBlock">
					<span class="username">
						<ui:msg description="Welcome">Welcome</ui:msg>
